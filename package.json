{
  "name": "autostory-ai-writer",
  "private": true,
  "version": "2.1.0",
  "description": "AI-powered Tistory content creation and automation tool",
  "repository": {
    "type": "git",
    "url": "https://github.com/DeclanJeon/autostory.git"
  },
  "author": "AutoStory Team",
  "packageManager": "pnpm@9.15.0",
  "main": "./dist/main/main.js",
  "scripts": {
    "dev": "concurrently \"vite\" \"pnpm build:main && pnpm electron\"",
    "build": "tsc -p tsconfig.main.json && vite build",
    "build:main": "tsc -p tsconfig.main.json",
    "electron": "cross-env VITE_DEV_SERVER_URL=http://localhost:3000 electron . --no-sandbox --disable-gpu",
    "electron:linux": "cross-env VITE_DEV_SERVER_URL=http://localhost:3000 electron . --no-sandbox --disable-setuid-sandbox --disable-gpu",
    "preview": "vite preview",
    "postinstall": "playwright install",
    "pack": "pnpm build && electron-builder --dir",
    "dist": "pnpm build && electron-builder",
    "dist:win": "pnpm build && electron-builder --win",
    "dist:linux": "pnpm build && electron-builder --linux",
    "dist:linux:debug": "pnpm build && electron-builder --linux --config.compression=store",
    "dist:mac": "pnpm build && electron-builder --mac",
    "dist:all": "pnpm build && electron-builder -mwl"
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "@napi-rs/canvas": "^0.1.86",
    "@types/cheerio": "^1.0.0",
    "cheerio": "^1.1.2",
    "electron-store": "^8.2.0",
    "extract-zip": "^2.0.1",
    "fs-extra": "^11.3.3",
    "lucide-react": "^0.561.0",
    "node-cron": "^4.2.1",
    "node-fetch": "^2.7.0",
    "pdfjs-dist": "^5.4.449",
    "playwright": "^1.57.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^7.11.0",
    "remark-gfm": "^4.0.1",
    "rss-parser": "^3.13.0",
    "tar": "^7.5.2",
    "tesseract.js": "^7.0.0",
    "turndown": "^7.2.2",
    "uuid": "^13.0.0",
    "winston": "^3.19.0",
    "youtube-transcript": "^1.2.1",
    "zustand": "^5.0.9"
  },
  "devDependencies": {
    "@types/node": "^22.19.3",
    "@types/node-cron": "^3.0.11",
    "@types/node-fetch": "^2.6.13",
    "@types/tar": "^6.1.13",
    "@types/turndown": "^5.0.6",
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-react": "^5.1.2",
    "concurrently": "^9.2.1",
    "cross-env": "^10.1.0",
    "electron": "^39.2.7",
    "electron-builder": "^26.0.12",
    "typescript": "~5.8.3",
    "vite": "^6.4.1"
  }
}
